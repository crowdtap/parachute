{
  "name":         "parachute",
  "version":      "0.0.1-alpha",
  "description":  "Simple assets delivery across your apps.",
  "author":       "Jico Baligod <jico@baligod.com>",
  "licenses": [
    {
      "type": "MIT",
      "url":  "http://github.com/jico/parachute/raw/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">=0.8.0"
  },
  "repository": {
    "type": "git",
    "url":  "https://github.com/jico/parachute"
  },
  "dependencies": {
    "colors":    "0.6.0-1",
    "gift":      "0.0.5",
    "hogan.js":  "2.0.0",
    "lodash":    "1.0.1",
    "mkdirp":    "0.3.5",
    "ncp":       "0.4.2",
    "nopt":      "2.1.1",
    "rimraf":    "2.1.4"
  },
  "devDependencies": {
    "coffee-script":  "1.6.2",
    "mocha":          "1.8.2",
    "expect.js":      "0.2.0"
  },
  "main": "lib",
  "bin": {
    "parachute": "bin/parachute"
  },
  "scripts": {
    "compile":  "./node_modules/.bin/coffee -bo . src",
    "test":     "./node_modules/.bin/mocha --reporter spec --timeout 40000",
    "watch":    "./node_modules/.bin/coffee -wbo . src"
  }
}
